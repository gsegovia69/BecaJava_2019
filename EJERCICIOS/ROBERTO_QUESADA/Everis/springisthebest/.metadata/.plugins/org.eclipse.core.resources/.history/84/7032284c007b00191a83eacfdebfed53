import lombok.ToString;

@Setter
@Getter
@ToString
public class AsignaturaDTO {

	private int idAsignatura;

	private String nombre;

	private int orden;
	
	private int idCurso;
	
	private String nombreCurso;
	
	private ClaseDTO idClase;

	private List<ClaseDTO> cursos;
		
		
	
}
