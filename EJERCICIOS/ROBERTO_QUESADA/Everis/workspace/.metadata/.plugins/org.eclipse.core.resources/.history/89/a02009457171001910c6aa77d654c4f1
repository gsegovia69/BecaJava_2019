package everis.beca.java.practicas.mavenPrueba;

import java.util.List;
import java.util.Set;

import javax.persistence.EntityManager;
import javax.persistence.TypedQuery;

import everis.beca.java.practicas.mavenPrueba.dto.Alumno;
import everis.beca.java.practicas.mavenPrueba.dto.Clase;

public class Pruebas {
	public static void main(String[] args) {
		EntityManager em = JpaUtil.getEntityManagerFactory().createEntityManager();

		Set<Alumno> query = em.createQuery("Select c FROM Clase c", Clase.class);
		List<Clase> results = query.getResultList();

	}
}
